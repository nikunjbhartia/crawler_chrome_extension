{
  "manifest_version": 2,

  "name": "Crawl Plugin",
  "description": "This extension will crawl a page",
  "version": "1.0",
   "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],

  "browser_action": {
   "default_icon": "icon16.png",
   "default_popup": "popup.html"
  },
    "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}